{  "version": 2,  "builds": [    {      "src": "**/*.html",      "use": "@vercel/static"    },    {      "src": "api/**/*.js",      "use": "@vercel/node"    }  ],  "routes": [    {      "src": "/api/(.*)",      "dest": "/api/$1"    },    {      "src": "/(.*)",      "dest": "/$1"    }  ],  "headers": [    {      "source": "/(.*)",      "headers": [        {          "key": "Cache-Control",          "value": "no-store, no-cache, must-revalidate, proxy-revalidate, max-age=0"        }      ]    }  ]}